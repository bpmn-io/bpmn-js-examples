{
  "name": "bpmn-js-example-properties-panel-next",
  "version": "0.0.0",
  "description": "A bpmn-js modeler + properties panel example",
  "main": "app/index.js",
  "scripts": {
    "all": "run-s bundle",
    "dev": "run-p bundle:less watch:js serve",
    "start": "run-s bundle serve",
    "serve": "sirv public --dev",
    "bundle": "run-s bundle:*",
    "bundle:less": "lessc src/app.less public/app.css",
    "bundle:js": "webpack -c webpack.config.js",
    "watch:js": "run-s \"bundle:js -- --watch\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bpmn-io/bpmn-js-examples",
    "directory": "properties-panel-next-extension"
  },
  "keywords": [
    "bpmnjs-example",
    "bpmn-js-properties-panel"
  ],
  "author": {
    "name": "Niklas Kiefer",
    "url": "https://github.com/pinussilvestrus"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@bpmn-io/bpmn-properties-panel": "github:bpmn-io/bpmn-properties-panel#v0.1.0",
    "@bpmn-io/properties-panel": "^0.2.0",
    "bpmn-js": "^8.7.1",
    "jquery": "^3.6.0",
    "min-dash": "^3.7.0",
    "preact": "^10.5.14"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.5",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.0",
    "less": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^4.0.2",
    "sirv-cli": "^1.0.12",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2"
  }
}
